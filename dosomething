<script>

jQuery(document).ready(function() {
	function shuffle(a) {
    var j, x, i;
    for (i = a.length - 1; i > 0; i--) {
			j = Math.floor(Math.random() * (i + 1));
			x = a[i];
			a[i] = a[j];
			a[j] = x;
    }
		return a;
	}

	function randomize(a) {
		var shuffled = shuffle(a);
		var i = 0

		return function() {
			if (i == shuffled.length) {
				shuffled = shuffle(a);
				i = 0;
			}

			return shuffled[i++]
		}
	}

	var getPrompt = randomize([{"prompt":"Work hard, snack often."},{"prompt":"Foodie with a booty."},{"prompt":"You are cute and you deserve a break."},{"prompt":"Making stacks & eating snacks."},{"prompt":"Forever bettering my lettering."},{"prompt":"Sassy & gassy."},{"prompt":"Live, love, loaf."},{"prompt":"Making art & breaking hearts."},{"prompt":"Big dreams, big heart, big butt."},{"prompt":"Saucy & bossy."},{"prompt":"You\u2019re the drunk to my pizza."},{"prompt":"My passport gets more action than I do."},{"prompt":"A big idea is like a big fart: you can\u2019t hold it in."},{"prompt":"Don\u2019t get mad. Get pizza."},{"prompt":"You\u2019re a deep dish in a world of dollar slices."},{"prompt":"May your dreams be as big as your appetite."},{"prompt":"Sipping wine and feeling fine."},{"prompt":"Bold but controlled."},{"prompt":"Be kind and unwind."},{"prompt":"My yogurt's more cultured than me."},{"prompt":"Loose lips sink bags of chips."},{"prompt":"Work hard, slay hard."},{"prompt":"I\u2019m in it to gin it."},{"prompt":"Treat work like cake: finish it."},{"prompt":"I\u2019d rather snooze than schmooze."},{"prompt":"Sweaty & petty."},{"prompt":"Home is where the fart is."},{"prompt":"Home is where the wine is."},{"prompt":"Swipe right on yourself."},{"prompt":"Now accepting praise in the form of a raise."},{"prompt":"Winter is coming. (That makes two of us). "},{"prompt":"Vibes are like white wine: better when chilled."},{"prompt":"Make like pie and get baked."},{"prompt":"Geeky & freaky."},{"prompt":"Keep your eyes on the fries."},{"prompt":"Feel the kern."},{"prompt":"That word is yearning for a kerning."},{"prompt":"Only here for beer."},{"prompt":"The only pot I'm stirring is this mac n' cheese."},{"prompt":"Always in the mood for food."},{"prompt":"It was love at first bite."},{"prompt":"I\u2019m not lazy. I\u2019m comfortable."},{"prompt":"Will doodle for noodles."},{"prompt":"Instagram is for snooping while pooping."},{"prompt":"Got a hunch for brunch."}